{
    "version": 3,
    "beforeBuildTasks": [
        {
            "name": "cmd.exe /C \"$P../../../../../../tools/keil/syscfg.bat '$P' empty.syscfg\"",
            "command": "cd .\\..\\keil && cmd.exe /C \"$P../../../../../../tools/keil/syscfg.bat '$P' empty.syscfg\"",
            "disable": false,
            "abortAfterFailed": true,
            "stopBuildAfterFailed": true
        }
    ],
    "afterBuildTasks": [],
    "global": {
        "use-microLIB": true,
        "output-debug-info": "enable"
    },
    "c/cpp-compiler": {
        "optimization": "level-2",
        "language-c": "c99",
        "language-cpp": "c++11",
        "link-time-optimization": false,
        "one-elf-section-per-function": true,
        "short-enums#wchar": true,
        "warnings": "misra-compatible"
    },
    "asm-compiler": {
        "$use": "asm-auto"
    },
    "linker": {
        "output-format": "elf",
        "misc-controls": "../../../../../../source/ti/driverlib/lib/keil/m0p/mspm0g1x0x_g3x0x/driverlib.a",
        "xo-base": "",
        "ro-base": "",
        "rw-base": ""
    }
}